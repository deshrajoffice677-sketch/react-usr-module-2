{
  "name": "new-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,json,css,md}\"",
    "typecheck": "tsc",
    "lint:staged": "lint-staged",
    "lint:all": "DEBUG=eslint:cli-engine eslint \"src/**/*.ts\" && DEBUG=eslint:cli-engine eslint \"src/**/*.tsx\"",
    "lint:all:fix": "DEBUG=eslint:cli-engine eslint \"src/**/*.ts\" --fix && DEBUG=eslint:cli-engine eslint \"src/**/*.tsx\" --fix",
    "prettier:check": "prettier 'src/**/*.{ts,tsx}' --check --ignore-path .prettierignore",
    "prettier:fix": "prettier 'src/**/*.tsx' --write --ignore-path .prettierignore",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "lint-staged": "^16.2.7",
    "lucide-react": "^0.555.0",
    "pnpm": "^10.24.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.10.1",
    "recharts": "^3.6.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "prettier": "3.7.4",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
